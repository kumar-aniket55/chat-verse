    <style>
        body { background-image: url(data:image/png;base64,);}
.error-template {padding: 40px 15px;text-align: center;    text-align: center !important;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;}
.error-actions {margin-top:15px;margin-bottom:15px;}
.error-actions .btn { margin-right:10px; }
.oops{
    text-align: center !important;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
body{
    margin: 0;
}
.button-two{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
.error{
    background-color: #ffb3c1 !important;
    width: 80%;
    margin: auto;
}
 .error p{
 font-size: 1.9rem;
}
.button-two a{
   margin: 16px;
}
@media screen and (max-width: 780px){
    .increase{
        width: 100% !important;
    }
    .button-two{
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }
    .error p{
        font-size: 1.1rem;
       }
       .error{
           width: 100%;
       }

    
}
    </style>
</head>
<body>
   
              <h2 style="text-align: center;">OOPS!</h2>
              <h3 style="text-align: center;">404 PAGE NOT FOUND</h3>
           <div class="button-two">
