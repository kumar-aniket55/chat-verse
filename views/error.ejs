                <button class="btn btn-primary btn-lg"><span class="glyphicon glyphicon-home"></span> TAKE ME TO HOME </button>
            </a>

           <a href="/contact" >
                <button class="btn btn-primary btn-lg"><span class="glyphicon glyphicon-envelope"></span>CONTACT SUPPORT</button>
            </a>
           </div>
           <div class="col-12 error" >
               <div class="row">
                   <div class="col-12">
                       <h1 style="text-align: center;"><%=err.message%></h1>
                       <p style="text-align: center;"><%= err.stack %></p>
                   </div>
               </div>
           </div>
        
        
